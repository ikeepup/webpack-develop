{
  "name": "vue-component-boilerplate",
  "version": "1.0.0",
  "description": "一个用于创建 vue 组件库的脚手架",
  "main": "./cjs/vue-component-boilerplate.js",
  "scripts": {
    "build": "webpack-cli --config ./build/webpack.common.js",
    "build-component": "webpack-cli --config ./build/webpack.component.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "deploy-local": "npm run build-storybook && http-server .out",
    "deploy-ghpages": "storybook-to-ghpages",
    "qiniu": "node ./bin/qiniu-deploy.js",
    "deploy-qiniu": "run-s build-storybook qiniu",
    "test": "run-s eslint jest",
    "jest": "jest",
    "generate": "./bin/generate.sh",
    "eslint": "eslint components --fix"
  },
  "keywords": [],
  "author": "应开翔",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@storybook/addon-actions": "^5.3.7",
    "@storybook/addon-docs": "^6.0.0-alpha.1",
    "@storybook/addon-links": "^5.3.7",
    "@storybook/addons": "^5.3.7",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/vue": "^5.3.7",
    "@types/jest": "^25.1.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "commitizen": "^4.0.3",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-team-spec": "^1.0.7",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-prettier-vue": "^2.0.2",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.0.2",
    "glob": "^7.1.6",
    "http-server": "^0.12.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "mime-types": "^2.1.26",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "qiniu": "^7.3.0",
    "sass-loader": "^8.0.2",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "vue": "^2.6.11",
    "vue-eslint-parser": "^7.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
